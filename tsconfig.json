{
  "compilerOptions": {
    "target": "ES2022",
    "baseUrl": ".",
    "paths": {
      "#components/*": ["./src/components/*"],
      "#constants/*": ["./src/constants/*"],
      "#store/*": ["./src/store/*"],
      "#hoc/*": ["./src/hoc/*"],
      "#windows/*": ["./src/windows/*"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": ["vite/client"],
    "forceConsistentCasingInFileNames": false,
    "skipLibCheck": true
  },
  "include": ["src"]
}
